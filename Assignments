Q20.	Print all prime numbers between two given numbers. [ break continue ]
=>
import java.util.Scanner;

public class Q20{
    public static void main(String[] args) {
        Scanner n1 = new Scanner(System.in);
       int x= n1.nextInt();
       int y= n1.nextInt();
       int flag=0;

       for(int i=x;i<=y;i++)
       {
           for(int j=2;j<i;j++)
           {
            flag=0;
               if(i%j == 0)
               {
                   break;
               }
               else
               {
                   flag=1;
                   continue;
               }
           }
           if(flag==1)
           {
               System.out.println(" "+i);
           }
       }
    }
}
