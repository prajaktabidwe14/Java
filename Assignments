=>
import java.util.Scanner;
class Q16
{
   public static void main(String args[])
   {
      Scanner sc = new Scanner(System.in);
     int no = sc.nextInt();
     
    for(int i=0;i<=10;i++)
    {
      rs=no*i;
      System.out.println(rs);
    }
   }
}

